{
  "name": "wile-extension",
  "type": "module",
  "private": true,
  "version": "0.0.1",
  "description": "Extension to play with Web Tiles",
  "author": "Robin Berjon <robin@berjon.com>",
  "scripts": {
    "watch": "esbuild wile.js --bundle --outfile=wile.min.js --format=esm --watch --sourcemap",
    "build": "esbuild wile.js --bundle --outfile=wile.min.js --format=esm"
  },
  "bin": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/darobin/wile.git"
  },
  "esm": {
    "await": true
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "mocha": true,
      "es2021": true
    },
    "extends": "eslint:recommended",
    "overrides": [],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {}
  },
  "devDependencies": {
    "esbuild": "^0.19.11",
    "eslint": "^8.26.0"
  },
  "dependencies": {
    "@helia/dag-cbor": "^1.0.3",
    "helia": "^2.1.0",
    "multiformats": "^13.0.0"
  }
}
